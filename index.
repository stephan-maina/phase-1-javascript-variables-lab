const myName = "Christiano Ronaldo"

// myName cannot be reassigned to a new value
myName = "Christiano Ronaldo"; // Error

let myAge = 19;

// myAge can be reassigned to a new value
myAge = 20;

let myVariable = "Hello World!";

console.log(myVariable); // Hello World!

myVariable = "Goodbye World!";

console.log(myVariable); // Goodbye World!

